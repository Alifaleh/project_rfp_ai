<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Tags -->
        <record id="tag_high_speed" model="rfp.ai.model.tag">
            <field name="name">High Speed</field>
            <field name="color">1</field>
        </record>
        <record id="tag_high_quality" model="rfp.ai.model.tag">
            <field name="name">High Quality</field>
            <field name="color">2</field>
        </record>
        <record id="tag_english" model="rfp.ai.model.tag">
            <field name="name">English Only</field>
            <field name="color">4</field>
        </record>
        <record id="tag_multilingual" model="rfp.ai.model.tag">
            <field name="name">Multilingual</field>
            <field name="color">3</field>
        </record>
        <record id="tag_image" model="rfp.ai.model.tag">
            <field name="name">Image</field>
            <field name="color">5</field>
        </record>

        <!-- Models -->
        <record id="model_gemini_flash" model="rfp.ai.model">
            <field name="name">Gemini 3 Pro Preview</field>
            <field name="technical_name">gemini-3-pro-preview</field>
            <field name="provider">google</field>
            <field name="tag_ids" eval="[(4, ref('tag_high_quality')), (4, ref('tag_english'))]"/>
            <field name="description">Google's most intelligent model with SOTA reasoning and multimodal understanding, and powerful agentic and vibe coding capabilities.</field>
        </record>

        <record id="model_gemini_flash_2_5" model="rfp.ai.model">
            <field name="name">Gemini 2.5 Flash</field>
            <field name="technical_name">gemini-2.5-flash</field>
            <field name="provider">google</field>
            <field name="tag_ids" eval="[(4, ref('tag_high_speed')), (4, ref('tag_english'))]"/>
            <field name="description">Google's hybrid reasoning model, with a 1M token context window and thinking budgets.</field>
        </record>

        <record id="model_gemini_flash_latest" model="rfp.ai.model">
            <field name="name">Gemini Flash Latest</field>
            <field name="technical_name">gemini-flash-latest</field>
            <field name="provider">google</field>
            <field name="tag_ids" eval="[(4, ref('tag_high_speed')), (4, ref('tag_english'))]"/>
            <field name="description">Google's hybrid reasoning model, with a 1M token context window and thinking budgets.</field>
        </record>

        <record id="model_gemini_flash_lite_latest" model="rfp.ai.model">
            <field name="name">Gemini Flash Lite Latest</field>
            <field name="technical_name">gemini-flash-lite-latest</field>
            <field name="provider">google</field>
            <field name="tag_ids" eval="[(4, ref('tag_high_speed')), (4, ref('tag_english'))]"/>
            <field name="description">Google's smallest and most cost effective model, built for at scale usage.</field>
        </record>

        <record id="model_imagen_4_0_generate_001" model="rfp.ai.model">
            <field name="name">Imagen 4.0</field>
            <field name="technical_name">imagen-4.0-generate-001</field>
            <field name="provider">google</field>
            <field name="tag_ids" eval="[(4, ref('tag_high_quality')), (4, ref('tag_image'))]"/>
            <field name="description">Google's Our latest image generation model, with significantly better text rendering and better overall image quality..</field>
        </record>

    </data>
</odoo>
